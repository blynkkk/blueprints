    {
        "id": "c50b31362d1d7f26",
        "type": "function",
        "z": "43a6f8e27a9d54a0",
        "name": "check_received_data",
        "func": "\nif (msg.topic == \"downlink/ds/Channel 1 Notification Threshold\") {\n    global.set(\"channel1_threshold\", msg.payload)\n} else if (msg.topic == \"downlink/ds/Channel 2 Notification Threshold\") {\n    global.set(\"channel2_threshold\", msg.payload)\n} else if (msg.topic == \"downlink/ds/Channel 3 Notification Threshold\") {\n    global.set(\"channel3_threshold\", msg.payload)\n} \n\n\n\n",
        "outputs": 0,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
